# vim: ft=dosini sw=4 et :

# Primary
[bar/DP-4]
inherit = bar/default
tray-position = right
modules-center = xwindow
modules-right = network-bit-rate ping vpn memory date

# Secondary
[bar/DP-0]
inherit = bar/default
modules-right = uptime cputemp cpu cpu-frequency system-fan-speed time

# Portrait
[bar/DP-2]
inherit = bar/default
modules-center = network
modules-right = power-profile fs-root time date
