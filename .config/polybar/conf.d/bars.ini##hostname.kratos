# vim: ft=dosini sw=4 et :

# Primary
[bar/DP-4]
inherit = bar/default
modules-center = xwindow
modules-right = network-bit-rate ping vpn memory date tray

# Left
[bar/DP-0]
inherit = bar/default
modules-right = uptime cputemp cpu cpu-frequency system-fan-speed time

# Right
[bar/DP-2]
inherit = bar/default
modules-center = network
modules-right = power-profile fs-root date time
