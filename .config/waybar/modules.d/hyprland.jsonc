{
  "hyprland/window": {
    "format": "{title:.50}",
    "icon": true,
    "separate-outputs": true,
    "icon-size": 20,
  },
  "hyprland/workspaces": {
    "format": "{name}  {icon}",
    "disable-scroll": false,
    "all-outputs": false,
    "show-special": true,
    "special-visible-only": false,
    "on-click": "activate",
    "format-icons": {
      "1": "󰈹",
      "2": "",
      "11": "󰈹",
      "12": "",
      "7": "",
      "8": "",
      "default": "󱂬",
      "special": "",
    },
  },
  "hyprland/submap": {
    "format": "✌️ {}",
    "max-length": 8,
    "tooltip": false,
  },
}
