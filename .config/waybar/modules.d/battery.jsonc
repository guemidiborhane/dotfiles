{
  "battery": {
    "states": {
      "warning": 30,
      "critical": 15,
    },
    "interval": 60,
    "full-at": 89,
    "format": "{icon}  {capacity}%",
    "tooltip-format": "{timeTo}\nPower draw: {power}W\nCycles: {cycles}",
    "format-icons": ["", "", "", "", ""],
  },
  "group/battery": {
    "orientation": "inherit",
    "modules": ["custom/power-status", "battery"],
  },
}
